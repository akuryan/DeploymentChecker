<?xml version="1.0"?>
<package>
  <metadata>
    <id>RobotsTxt.Checker</id>
    <version>1.0.0</version>
    <authors>Anton Kuryan</authors>
    <owners>Anton Kuryan</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Small utility, used to check robots.txt on web app and make sure that it is valid. RobotsTxt check is based on https://www.nuget.org/packages/RobotsTxt, but this project is no longer maintained, so I copied it for myself and introduced some useful (as I see them) improvements
    </description>
    <projectUrl>https://github.com/akuryan/DeploymentChecker/tree/master/src/Executables/RobotsTxt.Checker</projectUrl>
    <copyright>Copyright 2020</copyright>
    <tags>utility, robots.txt, check</tags>
    <language>en-US</language>
    <!-- NuGet package dependency -->
    <dependencies>
      <dependency id="CommandLineParser" version="2.7.82" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\..\publish\robotsChecker\**" target="tools" />
    <file src="readme.md" target="" />
  </files>
</package>